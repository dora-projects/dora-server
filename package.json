{
  "name": "dora-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist && prisma generate",
    "start": "concurrently \"npm run start:manager\" \"npm run start:process\" \"npm run start:relay\"",
    "start:manager": "cross-env NODE_ENV=development nest start manager --watch",
    "start:process": "cross-env NODE_ENV=development nest start process --watch",
    "start:relay": "cross-env NODE_ENV=development nest start relay --watch",
    ".": "",
    "debug:manager": "cross-env NODE_ENV=development nest start manager --debug 9003 --watch",
    "debug:process": "cross-env NODE_ENV=development nest start process --debug 9002 --watch",
    "debug:relay": "cross-env NODE_ENV=development nest start relay --debug 9001 --watch",
    "..": "",
    "build": "npm run build:manager && npm run build:process && npm run build:relay",
    "build:manager": "cross-env NODE_ENV=production nest build manager",
    "build:process": "cross-env NODE_ENV=production nest build process",
    "build:relay": "cross-env NODE_ENV=production nest build relay",
    "...": "",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --forceExit",
    "test:ui": "npx majestic",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "release": "release-it",
    "prisma:migrate": "prisma migrate dev",
    "prisma:format": "prisma format"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.14.1",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/elasticsearch": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.0",
    "@prisma/client": "^3.5.0",
    "ajv": "^8.8.2",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.1",
    "cache-manager": "^3.5.0",
    "cache-manager-redis-store": "^2.0.0",
    "chalk": "^4.1.2",
    "class-transformer": "~0.4.0",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^10.0.0",
    "html-minifier-terser": "^7.0.0-alpha.1",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.0",
    "nest-winston": "^1.6.2",
    "nodemailer": "^6.6.3",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "source-map": "^0.7.3",
    "swagger-ui-express": "^4.1.6",
    "ua-parser-js": "^0.7.28",
    "winston": "^3.3.3",
    "xprofiler": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/bull": "^3.15.4",
    "@types/cache-manager": "^3.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.176",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "commitizen": "^4.2.4",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "lint-staged": "^11.2.3",
    "majestic": "^1.8.1",
    "prettier": "^2.3.2",
    "prisma": "^3.5.0",
    "release-it": "^14.11.6",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{js,ts,json,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^libs/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^libs/datasource(|/.*)$": "<rootDir>/libs/datasource/src/$1"
    }
  }
}
